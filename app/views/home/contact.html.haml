        = succeed "," do
          %a{href: "#"} odio erat sagittis quam
      .cleaner_h50
      - if @home.errors.any?
        #error_explanation
          %h5
            = pluralize(@home.errors.count, "error") 
            ="prohibited this message to be sent"
            -@home.errors.full_messages.each do |msg|
              %li
                =msg
      #contact_form
        %h2 Contact Form
        =form_for(@home) do |f|
          %label{for: "author"} Name:
          =f.text_field :name, placeholder: "Enter your name...", class: "required input_field"
          .cleaner_h10
          %label{for: "email"} Email Address:
          =f.text_field :email, placeholder: "Enter your email...", class: "validate-email required input_field"
          .cleaner_h10
          %label{for: "text"} Message:
          =f.text_area :message, placeholder: "Write your message", class: "required"
          .cleaner_h10
          =f.submit "Send", class: "submit_btn"
          =f.submit "Reset Form", :type => "reset", class: "submit_btn"
        
    / end of main
    .cleaner
